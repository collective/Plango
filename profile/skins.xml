<?xml version="1.0"?>
<object name="portal_skins">

    <object name="tango_plone_images"
                meta_type="Filesystem Directory View"
                directory="Plango/skins/tango_plone_images"/>

    <object name="tango_ploneboard_images"
                meta_type="Filesystem Directory View"
                directory="Plango/skins/tango_ploneboard_images"/>

    <skin-path name="*">
        <layer name="tango_plone_images" insert-after="custom"/>
        <layer name="tango_ploneboard_images" insert-after="tango_plone_images"/>
        <layer name="tango_mimetypes_icons" insert-after="tango_ploneboard_images"/>
    </skin-path>

</object>